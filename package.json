{
  "name": "sqlapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "deleteNotUsedImports": "eslint --fix --ext .ts,.tsx src/",
    "prettyFiles": "prettier --write \"./**/*.{ts,tsx}\"",
    "pretty": "npm run deleteNotUsedImports",
    "podInstall": "pod install --project-directory=ios",
    "podIArch": "arch -x86_64 pod install --project-directory=ios",
    "firstInstallWindows": "npm i",
    "firstInstallMac": "npm run firstInstallWindows && npm run podInstall",
    "firstInstallMacM1": "npm run firstInstallWindows && npm run podIArch",
    "gen-graphql": "graphql-codegen --config codegen.ts",
    "build:android:adb": "cd android && ./gradlew bundleRelease && cd ..",
    "build:android:apk": "cd android && ./gradlew assembleRelease && cd ..",
    "regenerateKeystore": "cd android && cd app && rm -rf App_GQL.keystore && cd .. && cd .. && keytool -genkey -v -keystore App_GQL.keystore -alias D42yCK#J1y -keyalg RSA -keysize 2048 -validity 10000 && mv App_GQL.keystore android/app"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-native-community/netinfo": "^9.4.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/devtools": "^6.0.20",
    "@react-navigation/elements": "^1.3.21",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.16",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "luxon": "^3.4.3",
    "native-base": "^3.4.28",
    "react": "18.1.0",
    "react-native": "0.70.10",
    "react-native-bootsplash": "^5.0.4",
    "react-native-dotenv": "^3.4.7",
    "react-native-pure-jwt": "^3.0.2",
    "react-native-reanimated": "^2.14.0",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.27.0",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@graphql-codegen/cli": "5.0.0",
    "@graphql-codegen/client-preset": "4.1.0",
    "@graphql-codegen/fragment-matcher": "^5.0.0",
    "@graphql-codegen/typescript-react-apollo": "^4.1.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/luxon": "^3.3.3",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-unused-imports": "^3.0.0",
    "graphql-codegen": "^0.4.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "prettier": "^3.0.3",
    "react-native-flipper": "^0.212.0",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
